{
    "name": "nawarian/dumm",
    "description": "DOOM 1 ported to PHP with nawarian/raylib-ffi",
    "type": "project",
    "license": "GPL-3-or-later",
    "authors": [
        {
            "name": "Nawarian",
            "email": "nickolas@phpsp.org.br"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Nawarian\\Dumm\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Nawarian\\Dumm\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "nawarian/raylib-ffi": "dev-main",
        "mdurrant/php-binary-reader": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
